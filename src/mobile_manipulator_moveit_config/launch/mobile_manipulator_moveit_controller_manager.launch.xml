<launch>
    <!-- Set the param that trajectory_execution_manager needs to find the controller plugin -->
    <arg name="mobile_manipulator_controller_manager" default="moveit_simple_controller_manager/MoveItSimpleControllerManager" />
    <param name="mobile_manipulator_controller_manager" value="$(arg moveit_controller_manager)"/>
    <!-- load controller_list -->
    <rosparam file="$(find robot_moveit_config)/config/controllers.yaml"/>
</launch>
